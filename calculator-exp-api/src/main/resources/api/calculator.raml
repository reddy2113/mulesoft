#%RAML 1.0
title: calculator
version: v1
description: "this api is used to perform some math operations"

/additions:
  get:
    queryParameters:
      number1 :
        required: true
        type: integer
        example: 10
      number2 :
        required: true
        type: integer
        example: 30
    responses:
      200:
        body:
          application/json:
            example: !include Example/addition-responses.json
      400:
        description: |
          Bad result
      401:
        description: |
            Internal server error
      404:
        body:
          application/json:
            example:
              {
                "Messae": "Not Found"
              }
      500:
        body:
           application/json:
             example:
               {
                 "message" : "Internar Server error"
               }
      504:
        body:
          application/json:
            example:
              {
                "Message" : "Gateway Time Out"
              }
